[32m+[m
[32m+[m[32mpublic class NettyClient {[m
[32m+[m[32m    public static void main(String[] args) throws  Exception{[m
[32m+[m
[32m+[m[32m        //1. åˆ›å»ºä¸€ä¸ªçº¿ç¨‹ç»„[m
[32m+[m[32m        EventLoopGroup group=new NioEventLoopGroup();[m
[32m+[m[32m        //2. åˆ›å»ºå®¢æˆ·ç«¯çš„å¯åŠ¨åŠ©æ‰‹ï¼Œå®Œæˆç›¸å…³é…ç½®[m
[32m+[m[32m        Bootstrap b=new Bootstrap();[m
[32m+[m[32m        b.group(group)  //3. è®¾ç½®çº¿ç¨‹ç»„[m
[32m+[m[32m         .channel(NioSocketChannel.class)  //4. è®¾ç½®å®¢æˆ·ç«¯é€šé“çš„å®ç°ç±»[m
[32m+[m[32m         .handler(new ChannelInitializer<SocketChannel>() {  //5. åˆ›å»ºä¸€ä¸ªé€šé“åˆå§‹åŒ–å¯¹è±¡[m
[32m+[m[32m             @Override[m
[32m+[m[32m             protected void initChannel(SocketChannel socketChannel) throws Exception {[m
[32m+[m[32m                 socketChannel.pipeline().addLast(new NettyClientHandler()); //6.å¾€Pipelineé“¾ä¸­æ·»åŠ è‡ªå®šä¹‰çš„handler[m
[32m+[m[32m             }[m
[32m+[m[32m         });[m
[32m+[m[32m        System.out.println("......å®¢æˆ·ç«¯ å‡†å¤‡å°±ç»ª msgå‘å°„......");[m
[32m+[m
[32m+[m[32m        //7.å¯åŠ¨å®¢æˆ·ç«¯å»è¿æ¥æœåŠ¡å™¨ç«¯  connectæ–¹æ³•æ˜¯å¼‚æ­¥çš„   syncæ–¹æ³•æ˜¯åŒæ­¥é˜»å¡çš„[m
[32m+[m[32m        ChannelFuture cf=b.connect("127.0.0.1",9999).sync();[m
[32m+[m
[32m+[m[32m        //8.å…³é—­è¿æ¥(å¼‚æ­¥éé˜»å¡)[m
[32m+[m[32m        cf.channel().closeFuture().sync();[m
[32m+[m
[32m+[m[32m    }[m
