[32m+[m[32mimport io.netty.channel.socket.nio.NioServerSocketChannel;[m
[32m+[m
[32m+[m[32mpublic class NettyServer {[m
[32m+[m[32m    public static void main(String[] args) throws Exception{[m
[32m+[m[32m        //1. åˆ›å»ºä¸€ä¸ªçº¿ç¨‹ç»„ï¼šæ¥æ”¶å®¢æˆ·ç«¯è¿æ¥[m
[32m+[m[32m        EventLoopGroup bossGroup =new NioEventLoopGroup();[m
[32m+[m[32m        //2. åˆ›å»ºä¸€ä¸ªçº¿ç¨‹ç»„ï¼šå¤„ç†ç½‘ç»œæ“ä½œ[m
[32m+[m[32m        EventLoopGroup workerGroup =new NioEventLoopGroup();[m
[32m+[m[32m        //3. åˆ›å»ºæœåŠ¡å™¨ç«¯å¯åŠ¨åŠ©æ‰‹æ¥é…ç½®å‚æ•°[m
[32m+[m[32m        ServerBootstrap b=new ServerBootstrap();[m
[32m+[m[32m        b.group(bossGroup,workerGroup) //4.è®¾ç½®ä¸¤ä¸ªçº¿ç¨‹ç»„[m
[32m+[m[32m         .channel(NioServerSocketChannel.class) //5.ä½¿ç”¨NioServerSocketChannelä½œä¸ºæœåŠ¡å™¨ç«¯é€šé“çš„å®ç°[m
[32m+[m[32m         .option(ChannelOption.SO_BACKLOG,128) //6.è®¾ç½®çº¿ç¨‹é˜Ÿåˆ—ä¸­ç­‰å¾…è¿æ¥çš„ä¸ªæ•°[m
[32m+[m[32m         .childOption(ChannelOption.SO_KEEPALIVE,true) //7.ä¿æŒæ´»åŠ¨è¿æ¥çŠ¶æ€[m
[32m+[m[32m         .childHandler(new ChannelInitializer<SocketChannel>() {  //8. åˆ›å»ºä¸€ä¸ªé€šé“åˆå§‹åŒ–å¯¹è±¡[m
[32m+[m[32m             public void initChannel(SocketChannel sc){   //9. å¾€Pipelineé“¾ä¸­æ·»åŠ è‡ªå®šä¹‰çš„handlerç±»[m
[32m+[m[32m                 sc.pipeline().addLast(new NettyServerHandler());[m
[32m+[m[32m             }[m
[32m+[m[32m         });[m
[32m+[m[32m        System.out.println("......æœåŠ¡ç«¯ å¯åŠ¨ä¸­ init port:9999 ......");[m
[32m+[m[32m        ChannelFuture cf=b.bind(9999).sync();  //10. ç»‘å®šç«¯å£ bindæ–¹æ³•æ˜¯å¼‚æ­¥çš„  syncæ–¹æ³•æ˜¯åŒæ­¥é˜»å¡çš„[m
[32m+[m[32m        System.out.println("......æœåŠ¡ç«¯ å¯åŠ¨æˆåŠŸ ......");[m
[32m+[m
[32m+[m[32m        //11. å…³é—­é€šé“ï¼Œå…³é—­çº¿ç¨‹ç»„[m
[32m+[m[32m        cf.channel().closeFuture().sync(); //å¼‚æ­¥[m
[32m+[m[32m        bossGroup.shutdownGracefully();[m
[32m+[m[32m        workerGroup.shutdownGracefully();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
